<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.relativelayoutapplication">

    <application
        android:allowBackup="true"
        android:icon="@drawable/health"
        android:label="@string/app_name"
        android:roundIcon="@drawable/health"
        android:supportsRtl="true"
        android:theme="@style/Theme.RelativeLayoutApplication">
        <activity android:name=".ui.PesoActivity"/>
        <activity android:name=".ui.DashboardActivity"/>
        <activity android:name=".ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.NovoUsuarioActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>